# ALIASES

source ${HOME}/.zsh_aliases

# PROMPT
autoload -U promptinit && promptinit
prompt pure

# PLUGINS

# syntax highlighting

source ${HOME}/.dotfiles/zsh/plugins/syntax/zsh-syntax-highlighting.zsh

# autojump

[[ -s $(brew --prefix)/etc/profile.d/autojump.sh ]] && . $(brew --prefix)/etc/profile.d/autojump.sh
